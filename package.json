{
  "name": "zelus",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "dotenv -e .env.local -- react-router dev",
    "dev:staging": "dotenv -e .env.staging -- react-router dev",
    "dev:production": "dotenv -e .env.production -- react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "format": "pretty-quick",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "react-router typegen && tsc",
    "db:generate": "bun run scripts/db.ts generate",
    "db:migrate": "dotenv -e .env.local -- bun run scripts/db.ts migrate",
    "db:migrate:staging": "dotenv -e .env.staging -- bun run scripts/db.ts migrate --yes",
    "db:migrate:prod": "dotenv -e .env.production -- bun run scripts/db.ts migrate --yes",
    "db:check": "bun run scripts/db.ts check",
    "db:status": "bun run scripts/db.ts status",
    "db:seed": "dotenv -e .env.local -- bun run scripts/seed.ts",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.5",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-router/node": "7.12.0",
    "@react-router/remix-routes-option-adapter": "^7.13.0",
    "@react-router/serve": "7.12.0",
    "@vercel/blob": "^2.2.0",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "isbot": "^5.1.31",
    "motion": "^12.34.0",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-router": "7.12.0",
    "remix-flat-routes": "^0.8.5",
    "shadcn": "^3.8.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@react-router/dev": "7.12.0",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.6.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "pretty-quick": "^4.2.2",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.1.7",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.18"
  }
}
